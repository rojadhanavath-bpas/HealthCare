}



                                        @Html.Grid(Model).Columns(columns =>
                           {

                               columns.Add(m => m.Hospital_Account_ID).Titled(" ID ").Sortable(true).SortInitialDirection(GridMvc.Sorting.GridSortDirection.Ascending).Filterable(true);
                               columns.Add(m => m.Account_Date).Titled("DUE DATE").Format("{0:dd MMM yyyy}").Sortable(true).Filterable(true);
                               columns.Add(m => m.Account_Bill_Status).Titled("PRIORITY").Sortable(true).Filterable(true);
                               columns.Add(m => m.Reporting_Rsn_Code_w__Desc).Titled("DESCRIPTION").Sortable(true).Filterable(true);
                               columns.Add(m => m.Account_Patient_Name).Titled("OWNER").Sortable(true).Filterable(true);
                               columns.Add(m => m.Ext_Rsn_Code_w__Desc).Titled("COMMENT").Sortable(true).Filterable(true);
                               columns.Add().Titled("EDIT")
                               .Sanitized(false)
                               .Encoded(false)
                               .RenderValueAs(m => Html.ActionLink(" ", " ", new { EditRec_ID = m.Hospital_Account_ID }, new { @class = "btn-sm glyphicon glyphicon-pencil modal-link", @data_toggle = "modal", @data_target = "#editModal" }));
                               columns.Add().Titled("DELETE")
