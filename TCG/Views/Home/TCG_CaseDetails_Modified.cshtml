@*@model List<HealthcareAnalytics.Models.Get_Account_Info_for_ARandDenial_Result>*@
@model List<HealthcareAnalytics.Models.Account_Case_Task>
@using GridMvc.Html
@using HealthcareAnalytics.Models
@{
    ViewBag.Title = "TCG - Case Detials";
    WebGrid grid = new WebGrid(Model, rowsPerPage: 10);
}

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title> Index </title>

    <!-- jQuery UI CSS Reference -->
    <link href="@Url.Content("~/Content/themes/base/minified/jquery-ui.min.css")" rel="stylesheet" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap-responsive.min.css" rel="stylesheet" type="text/css" />

    <!-- Required jQuery and jQuery UI Javascript references -->
    <script src="~/scripts/jquery-3.2.1.min.js" type="text/javascript"></script>
    <script src="~/scripts/bootstrap.min.js" type="text/javascript"></script>
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js" type="text/javascript"></script>
    <script src="~/scripts/twitter-bootstrap.js" type="text/javascript"></script>

    <link href="@Url.Content("~/Content/Gridmvc.css")" rel="stylesheet" />
    <script src="@Url.Content("~/Scripts/gridmvc.min.js")"></script>


    @section Scripts {
        @Scripts.Render("~/bundles/jqueryval")

        <script type="text/javascript">
            $(function () {
                // $.fn.datepicker.defaults.format = "mm/dd/yyyy";
                $('.datepicker').datepicker({
                    calendarWeeks: true,
                    todayHighlight: true,
                    autoclose: true
                });
            });



        </script>
    }
    <script type="text/javascript">


        $(function () {
            $('body').on('click', '.modal-link', function (e) {
                e.preventDefault();
                $(this).attr('data-target', '#modal-container1');
                $(this).attr('data-toggle', 'modal');
            });

            $('body').on('click', '.close', function () {
                $('#modal-container1').modal('hide');
            });

            $('#modal-container1').on('hidden.bs.modal', function () {
                $(this).removeData('bs.modal');
            });

        });


        $(function () {
            $('body').on('click', '.modal-CreateLink', function (e) {
                e.preventDefault();
                $(this).attr('data-target', '#modal-container-create');
                $(this).attr('data-toggle', 'modal');
            });

            $('body').on('click', '.close', function () {
                $('#modal-container-create').modal('hide');
            });

            $('#modal-container-create').on('hidden.bs.modal', function () {
                $(this).removeData('bs.modal');
            });

        });


    </script>

</head>
<body>


    @using (Html.BeginForm("TCG_CaseDetails_Modified", "Home", FormMethod.Get))
    {
        @Html.AntiForgeryToken()
        <form>


            <div class="container">
                <div class="panel panel-body">
                    <div class="form-row">
                        @foreach (Account_Case_Details item in ViewBag.ACD_data)
                        {
                            <div class="form-group col-sm-4">
                                @Html.ActionLink("BACK", "prioirty_CaseList", new { id = @item.ACD_HspAccID }, new { @class = "btn btn-primary" })
                            </div>
                            <div class="form-group col-sm-4" style="float:right;">
                                <label for="Txt_CaseID">CASE ID : </label>
                                <span>@item.ACD_ID</span>
                            </div>
                            <div class="form-group col-sm-4" style="float:right;">
                                <label for="Txt_AccID">ACCOUNT ID : </label>
                                <span>@item.ACD_HspAccID</span>
                            </div>
                        }
                    </div>
                    <div class="form-row">
                        <div class="form-group col-sm-12">
                            <ul class="nav nav-tabs">
                                <li class="active"><a data-toggle="tab" class="nav_link" data_id="1" href="#id_cases_main">CASES</a></li>
                                @*<li class="nav-item"><a data-toggle="tab" class="nav_link" data_id="2" href="#id_openTask">OPEN TASK</a></li>
                                    <li class="nav-item"><a data-toggle="tab" class="nav_link" data_id="3" href="#id_account">ACCOUNT</a></li>*@
                                <li class="nav-item"><a data-toggle="tab" class="nav_link" data_id="3" href="#id_insuranceBuckets">INSURANCE BUCKETS</a></li>
                                <li class="nav-item"><a data-toggle="tab" class="nav_link" data_id="3" href="#id_caseHistory">CASE HISTORY</a></li>
                                <li class="nav-item"><a data-toggle="tab" class="nav_link" data_id="3" href="#id_Contacts">CONTACTS</a></li>
                            </ul>

                            <div class="tab-content">

                                <div id="id_cases_main" class="panel-body tab-pane fade in active">

                                    <div id="id_cases" class="panel-body">
                                        <div style="border:3px solid #fcbf49; background-color:#fcbf49">
                                            <h4>
                                                <span style=" font-size:large">
                                                    CASE <button class="btn glyphicon glyphicon-forward" id="btn_CaseDetails" OnClick="myFunction('id_cases_details');return false;"></button>
                                                </span>                                                
                                            </h4>
                                        </div>
                                        <hr />
                                        <div id="id_cases_details" style="display:block;">


                                            <div class="form-row">
                                                @foreach (Account_Case_Details item in ViewBag.ACD_data)
                                                {
                                                    <div class="form-group col-sm-4">
                                                        <label for="DDL_Status">Status</label>                                                       
                                                        @Html.DropDownListFor(Model => @item.ACD_Status, (IEnumerable<SelectListItem>)ViewBag.SM, "Select", new { @class = "form-control dropdown", @disabled = "disabled" })
                                                    </div>

                                                    <div class="form-group col-sm-4">
                                                        <label for="DDL_Owner">Owner</label>
                                                        @Html.DropDownListFor(Model => @item.ACD_Owner, (IEnumerable<SelectListItem>)ViewBag.UL, "Select", new { @class = "form-control dropdown", @disabled = "disabled" })                                                        
                                                    </div>
                                                }
                                                @foreach (HealthcareAnalytics.Models.Get_Account_Info_for_ARandDenial_Result s in ViewBag.OriginalData)
                                                {
                                                    <div class="form-group col-sm-2">
                                                        <label for="lbl_DeniedDays">Denied Days</label>
                                                        @Html.TextBox("0", 0, new { @class = "form-control", @readonly = "readonly" })
                                                    </div>

                                                    <div class="form-group col-sm-2">
                                                        <label for="lbl_RelatedCase">Related Case</label>
                                                        @Html.TextBox("0", 0, new { @class = "form-control", @readonly = "readonly" })
                                                    </div>
                                                }
                                            </div>


                                            <div class="form-row">
                                                @foreach (HealthcareAnalytics.Models.Get_Account_Info_for_ARandDenial_Result s in ViewBag.OriginalData)
                                                {

                                                    <div class="form-group col-sm-4">
                                                        <label for="dt_Date">Date</label>
                                                        @Html.TextBoxFor(Model => s.Coding_Date, new { @Value = s.Coding_Date.ToString(), @class = "form-control", @readonly="readonly" })
                                                    </div>
                                                }
                                                @foreach (Account_Case_Details item in ViewBag.ACD_data)
                                                {
                                                    <div class="form-group col-sm-4">
                                                        <label for="DDL_Type">Type</label>
                                                        @Html.DropDownListFor(Model => @item.ACD_Type, (IEnumerable<SelectListItem>)ViewBag.ECT, "Select", new { @class = "form-control dropdown", @disabled = "disabled" })                                                        

                                                    </div>
                                                    <div class="form-group col-sm-4">
                                                        <label for="DDL_PayerReason">Payer Reason</label>
                                                        @Html.DropDownListFor(Model => @item.ACD_PayerReason, (IEnumerable<SelectListItem>)ViewBag.PR, "Select", new { @class = "form-control dropdown", @disabled = "disabled" })                                                        
                                                    </div>
                                                }
                                            </div>


                                            <div class="form-row">
                                                @foreach (Account_Case_Details item in ViewBag.ACD_data)
                                                {
                                                    <div class="form-group col-sm-4">
                                                        <label for="txt_Amount">Amount</label>                                                        
                                                        @Html.TextBoxFor(Model => @item.ACD_Amount, new { @class = "form-control ", @readonly = "readonly" })
                                                    </div>

                                                    <div class="form-group col-sm-4">
                                                        <label for="DDL_SubType">Sub-Type</label>
                                                        @Html.DropDownListFor(Model => @item.ACD_SubType, (IEnumerable<SelectListItem>)ViewBag.SbT, "Select", new { @class = "form-control dropdown", @disabled = "disabled" })                                                        
                                                    </div>

                                                    <div class="form-group col-sm-4">
                                                        <label for="DDL_PrimaryReason">Primary Reason</label>
                                                        @Html.DropDownListFor(Model => @item.ACD_PrimaryReason, (IEnumerable<SelectListItem>)ViewBag.PRM, "Select", new { @class = "form-control dropdown", @disabled = "disabled" })                                                        
                                                    </div>
                                                }
                                            </div>


                                            <div class="form-row">
                                                @foreach (Account_Case_Details item in ViewBag.ACD_data)
                                                {
                                                    <div class="form-group col-sm-8">
                                                        <label for="txt_Note">Note</label>                                                        
                                                        @Html.TextAreaFor(Model => @item.ACD_Comments, new { @class = "form-control ", @readonly = "readonly" })
                                                    </div>

                                                    <div class="form-group col-sm-4">
                                                        <label for="DDL_SecondaryReason">Secondary Reason</label>
                                                        @Html.DropDownListFor(Model => @item.ACD_SecondaryReason, (IEnumerable<SelectListItem>)ViewBag.SRM, "Select", new { @class = "form-control dropdown", @disabled = "disabled" })
                                                    </div>
                                                }
                                            </div>
                                        </div>
                                    </div>

                                    <div id="id_openTask" class="panel-body ">
                                        <div style="border:3px solid #fcbf49; background-color:#fcbf49">

                                            <h4>
                                                <span style=" font-size:large">
                                                    OPEN TASKS <button class="btn glyphicon glyphicon-forward" id="btn_openTask" OnClick="myFunction('id_OpenTask_Detials');return false;"></button>   
                                                </span>
                                            </h4>

                                        </div>
                                        <hr />
                                        <div id="id_OpenTask_Detials" style="display:block;">

                                            <div class="panel panel-default">
                                                @foreach (Account_Case_Details item in ViewBag.ACD_data)
                                                {
                                                    @Html.ActionLink("CREATE", "CreateNewTask_Details", new { id = item.ACD_HspAccID, case_ID = item.ACD_ID, task_ID = 0 }, new { @class = "btn btn-danger glyphicon glyphicon-plus-sign", @style = "float:left;" })
                                                }
                                                <div>
                                                    @Html.Grid(Model).Columns(columns =>
                                               {
                                                   columns.Add(m => m.ACT_ID).Titled(" ID ").Sortable(true).SortInitialDirection(GridMvc.Sorting.GridSortDirection.Ascending).Filterable(true);
                                                   columns.Add(m => m.ACT_DueDate).Titled("DUE DATE").Format("{0:dd MMM yyyy}").Sortable(true).Filterable(true);
                                                   columns.Add(m => m.ACT_Priority).Titled("PRIORITY").Sortable(true).Filterable(true);
                                                   columns.Add(m => m.ACT_Description).Titled("DESCRIPTION").Sortable(true).Filterable(true);
                                                   columns.Add(m => m.ACT_Owner).Titled("OWNER").Sortable(true).Filterable(true);
                                                   columns.Add(m => m.ACT_Comment).Titled("COMMENT").Sortable(true).Filterable(true);
                                                   columns.Add().Titled("COMPLETED")
                                                   .Encoded(false)
                                                   .Sanitized(false).SetWidth(30)
                                                   .RenderValueAs(o => Html.CheckBox("Selected", o.ACT_Completed, new { disabled = "disabled" }));
                                                   columns.Add().Titled("EDIT")
                                                   .Sanitized(false)
                                                   .Encoded(false)
                                                   .RenderValueAs(m => Html.ActionLink(" ", "EditOpenTask_Details", new { id = m.ACT_HspAccID, case_ID = m.ACT_ACD_ID, task_ID = m.ACT_ID }, new { @class = ".modal-link btn-sm glyphicon glyphicon-pencil" }));
                                                   columns.Add().Titled("DELETE")
                                                   .Sanitized(false)
                                                   .Encoded(false)
                                                   .RenderValueAs(m => Html.ActionLink(" ", "DeleteOpenTask", new { id = m.ACT_HspAccID, case_ID = m.ACT_ACD_ID, task_ID = m.ACT_ID }, new { @class = " btn-sm glyphicon glyphicon-trash" }));

                                               }).WithPaging(3).Sortable(true)
                                                </div>


                                            </div>


                                        </div>
                                    </div>

                                    <div id="id_account" class="panel-body">
                                        <div style="border:3px solid #fcbf49; background-color:#fcbf49">
                                            <h4> <span style=" font-size:large">ACCOUNT <button class="btn glyphicon glyphicon-forward" id="btn_Account" OnClick="myFunction('id_Accout_Detials');return false;"></button></span></h4>
                                        </div>
                                        <hr />
                                        <div id="id_Accout_Detials" style="display:block;">

                                            <div class="form-row">
                                                <div class="form-group col-sm-12">
                                                    @foreach (HealthcareAnalytics.Models.Get_Account_Info_for_ARandDenial_Result s in ViewBag.OriginalData)
                                                    {
                                                        <div class="form-row">
                                                            <div class="form-group col-sm-4">
                                                                <label for="Txt_AccNo">Account Number</label>
                                                                @Html.TextBoxFor(Model => @s.Hospital_Account_ID, new { @class = "form-control ", @readonly = "readonly" })
                                                            </div>

                                                            <div class="form-group col-sm-4">
                                                                <label for="Txt_AdmitDate">Admit Date</label>                                                                
                                                                @Html.TextBox(" ", " ", new { @class = "form-control ", @readonly = "readonly" })
                                                            </div>

                                                            <div class="form-group col-sm-4">
                                                                <label for="Txt_AccStatus">Account Status</label>
                                                                <input class="form-control" type="text" id="Txt_AccStatus" readonly />
                                                            </div>
                                                        </div>

                                                        <div class="form-row">
                                                            <div class="form-group col-sm-4">
                                                                <label for="Txt_PatientName">Patient Name</label>
                                                                @Html.TextBoxFor(Model => @s.Account_Patient_Name, new { @class = "form-control ", @readonly = "readonly" })
                                                            </div>

                                                            <div class="form-group col-sm-4">
                                                                <label for="Txt_DischargeDate">Discharge Date</label>
                                                                @Html.TextBoxFor(Model => @s.Discharge_Date, new { @Value = s.Discharge_Date.ToString(), @class = "form-control", @readonly = "readonly" })
                                                            </div>

                                                            <div class="form-group col-sm-4">
                                                                <label for="Txt_PatientType">Patient Type</label>
                                                                @Html.TextBoxFor(Model => @s.Encounter_Type, new { @class = "form-control ", @readonly = "readonly" })
                                                            </div>
                                                        </div>

                                                        <div class="form-row">
                                                            <div class="form-group col-sm-4">
                                                                <label for="Txt_MRN">MRN</label>
                                                                <input class="form-control" type="text" id="Txt_MRN" readonly />
                                                            </div>

                                                            <div class="form-group col-sm-4">
                                                                <label for="Txt_AdmitDX">Admit DX</label>
                                                                <input class="form-control" type="text" id="Txt_AdmitDX" readonly />
                                                            </div>

                                                            <div class="form-group col-sm-4">
                                                                <label for="Txt_Service">Service</label>
                                                                <input class="form-control" type="text" id="Txt_Service" readonly />
                                                            </div>
                                                        </div>

                                                    }
                                                    <div class="form-row">
                                                        @foreach (HealthcareAnalytics.Models.Get_Account_Info_for_ARandDenial_Result s in ViewBag.OriginalData)
                                                        {
                                                            <div class="form-group col-sm-4">
                                                                <label for="Txt_SSN">SSN</label>
                                                                <input class="form-control" type="text" id="Txt_SSN" readonly />
                                                            </div>
                                                        }
                                                        @foreach (Account_Case_Details item in ViewBag.ACD_data)
                                                        {
                                                            <div class="form-group col-sm-4">
                                                                <label for="DDL_PrinDiagOthers">Prin Diag/Others</label>
                                                                @Html.DropDownListFor(Model => @item.ACD_PrinDiag, (IEnumerable<SelectListItem>)ViewBag.PD, "Select", new { @class = "form-control dropdown", @disabled = "disabled" })
                                                            </div>

                                                            <div class="form-group col-sm-4">
                                                                <label for="DDL_PrinProcOthers">Prin Proc/Others</label>
                                                                @Html.DropDownListFor(Model => @item.ACD_PrinProc, (IEnumerable<SelectListItem>)ViewBag.PP, "Select", new { @class = "form-control dropdown", @disabled = "disabled" })
                                                            </div>
                                                        }
                                                    </div>
                                                    @foreach (HealthcareAnalytics.Models.Get_Account_Info_for_ARandDenial_Result s in ViewBag.OriginalData)
                                                    {
                                                        <div class="form-row">
                                                            <div class="form-group col-sm-4">
                                                                <label for="Txt_DOB">D.O.B</label>
                                                                <input class="form-control" type="text" id="Txt_DOB" readonly />
                                                            </div>

                                                            <div class="form-group col-sm-4">
                                                                <label for="Txt_Department">Department</label>
                                                                @Html.TextBoxFor(Model => @s.Account_Department_Name, new { @class = "form-control ", @readonly = "readonly" })
                                                            </div>

                                                            <div class="form-group col-sm-4">
                                                                <label for="Txt_Registration">Registration</label>
                                                                <input class="form-control" type="text" id="Txt_Registration" readonly />
                                                            </div>
                                                        </div>

                                                        <div class="form-row">
                                                            <div class="form-group col-sm-4">
                                                                <label for="Txt_Sex">SEX</label>
                                                                <input class="form-control" type="text" id="Txt_Sex" readonly />
                                                            </div>

                                                            <div class="form-group col-sm-4">
                                                                <label for="Txt_DRG">DRG</label>
                                                                <input class="form-control" type="text" id="Txt_DRG" readonly />
                                                            </div>

                                                            <div class="form-group col-sm-4">
                                                                <label for="Txt_RefPhysician">Ref. Physician</label>
                                                                <input class="form-control" type="text" id="Txt_RefPhysician" readonly />
                                                            </div>
                                                        </div>

                                                        <div class="form-row">
                                                            <div class="form-group col-sm-4">
                                                                <label for="Txt_FinancialClass">Financial Class</label>
                                                                <input class="form-control" type="text" id="Txt_FinancialClass" readonly />
                                                            </div>

                                                            <div class="form-group col-sm-4">
                                                                <label for="Txt_ContractID">Contract ID</label>
                                                                <input class="form-control" type="text" id="Txt_ContractID" readonly />
                                                            </div>

                                                            <div class="form-group col-sm-4">
                                                                <label for="Txt_AttdPhysician">Attd. Physician</label>
                                                                <input class="form-control" type="text" id="Txt_AttdPhysician" readonly />
                                                            </div>
                                                        </div>

                                                        <div class="form-row">
                                                            <div class="form-group col-sm-4">
                                                                <label for="Txt_CurrentFC">Current F/C</label>
                                                                <input class="form-control" type="text" id="Txt_CurrentFC" readonly />
                                                            </div>

                                                            <div class="form-group col-sm-4">
                                                                <label for="Txt_DischargeDispos">Discharge Disposition</label>
                                                                <input class="form-control" type="text" id="Txt_DischargeDispos" readonly />
                                                            </div>

                                                            <div class="form-group col-sm-4">
                                                                <label for="Txt_OperPhysician">Oper. Physician</label>
                                                                <input class="form-control" type="text" id="Txt_OperPhysician" readonly />
                                                            </div>
                                                        </div>
                                                    }
                                                </div>
                                            </div>

                                            <hr />

                                            <div class="form-row">

                                                <div class="form-group col-sm-6">
                                                    <div class="panel panel-default pre-scrollable">
                                                        <table class="table table-striped table-bordered table-responsive table-condensed table-hover">
                                                            <thead style="align-content:center; align-items:center; text-align:center">
                                                                <tr colspan="7" style="align-content:center; align-items:center">
                                                                    <th colspan="7" style="background-color:#fcbf49; align-content:center; align-items:center"> TRANSACTIONS</th>
                                                                </tr>
                                                                <tr style="align-content:center; align-items:center">
                                                                    <th></th>
                                                                    <th>DATE</th>
                                                                    <th>USER</th>
                                                                    <th>TYPE</th>
                                                                    <th>CODE</th>
                                                                    <th>AMOUNT</th>
                                                                    <th>W/O</th>
                                                                </tr>
                                                            </thead>
                                                            <tbody data-bind="foreach:Persons">
                                                                <tr>
                                                                    <td>
                                                                        <input name="chkbox" type="checkbox" />
                                                                    </td>
                                                                    <td>
                                                                        <span data-bind="text:Date"></span>
                                                                    </td>
                                                                    <td>
                                                                        <span data-bind="text:User"></span>
                                                                    </td>
                                                                    <td>
                                                                        <span data-bind="text:Type"></span>
                                                                    </td>
                                                                    <td>
                                                                        <span data-bind="text:Code"></span>
                                                                    </td>
                                                                    <td>
                                                                        <span data-bind="text:Amount"></span>
                                                                    </td>
                                                                    <td>
                                                                        <span data-bind="text:W/O"></span>
                                                                    </td>
                                                                </tr>
                                                            </tbody>
                                                        </table>
                                                    </div>
                                                </div>
                                                <div class="form-group col-sm-6">
                                                    <table class="table table-striped table-bordered table-responsive table-condensed table-hover">
                                                        <tbody data-bind="foreach:Persons">
                                                            <tr>
                                                                <td>
                                                                    <label for="Txt_FinalBillDate">First Bill Date</label>
                                                                </td>
                                                                <td>
                                                                    <input class="form-control" type="text" id="Txt_FinalBillDate" readonly />
                                                                </td>
                                                            </tr>
                                                            <tr>
                                                                <td>
                                                                    <label for="Txt_LastPayDate">Last Pay Date</label>
                                                                </td>
                                                                <td>
                                                                    <input class="form-control" type="text" id="Txt_LastPayDate" readonly />
                                                                </td>
                                                            </tr>
                                                            <tr>
                                                                <td>
                                                                    <label for="Txt_TotalCharges">Total Charges</label>
                                                                </td>
                                                                <td>
                                                                    <input class="form-control" type="text" id="Txt_TotalCharges" readonly />
                                                                </td>
                                                            </tr>
                                                            <tr>
                                                                <td>
                                                                    <label for="Txt_TotalPayments">Total payments</label>
                                                                </td>
                                                                <td>
                                                                    <input class="form-control" type="text" id="Txt_TotalPayments" readonly />
                                                                </td>
                                                            </tr>
                                                            <tr>
                                                                <td>
                                                                    <label for="Txt_TotalAdjustments">Total Adjustments</label>
                                                                </td>
                                                                <td>
                                                                    <input class="form-control" type="text" id="Txt_TotalAdjustments" readonly />
                                                                </td>
                                                            </tr>
                                                            <tr>
                                                                <td>
                                                                    <label for="Txt_TotalReferrals">Total Referals</label>
                                                                </td>
                                                                <td>
                                                                    <input class="form-control" type="text" id="Txt_TotalReferrals" readonly />
                                                                </td>
                                                            </tr>
                                                            <tr>
                                                                <td>
                                                                    <label for="Txt_CurrentBalance">Current Balance</label>
                                                                </td>
                                                                <td>
                                                                    <input class="form-control" type="text" id="Txt_CurrentBalance" readonly />
                                                                </td>
                                                            </tr>
                                                        </tbody>
                                                    </table>
                                                </div>

                                            </div>

                                        </div>
                                    </div>

                                </div>


                                <div id="id_insuranceBuckets" class="panel-body tab-pane fade">
                                    <div style="border:3px solid #fcbf49; background-color:#fcbf49">
                                        <h4> <span style=" font-size:large">INSURANCE BUCKETS <button class="btn glyphicon glyphicon-forward" id="btn_InsuranceBkt" OnClick="myFunction('id_InsuranceBuckets_detials');return false;"></button></span></h4>
                                    </div>
                                    <hr />
                                    <div id="id_InsuranceBuckets_detials">
                                        <div class="panel panel-default pre-scrollable">
