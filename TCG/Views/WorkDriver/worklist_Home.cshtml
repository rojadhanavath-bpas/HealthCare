
@{
    ViewBag.Title = "TCG - Worklist";
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>  </title>
        <!-- jQuery UI CSS Reference -->
   
</head>
<body>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()
        <form>
            <div class="container">
                <div class="panel panel-body">
                    <div id="id_cases" class="panel-body tab-pane fade in active">

                        <div style="border:3px solid #fcbf49; background-color:#fcbf49">
                            <h4>
                                <span style=" font-size:large">
                                    Change to Root Cause Review
                                </span>
                            </h4>
                        </div>
                        <hr />

                        <div>
                            <ul class="list-group">
                                <li class="list-group-item list-group-item-info"><i class="fa fa-chevron-circle-right"></i> &nbsp;@Html.ActionLink("PRIORITY 1 - Underpayments ", "Underpayemnts_UserAcc_List", "WorkDriver") </li>
                                <li class="list-group-item list-group-item-info"><i class="fa fa-chevron-circle-right"></i> &nbsp;@Html.ActionLink("PRIORITY 2 - Denials", "UserAcc_List", "WorkDriver") </li>
                                <li class="list-group-item list-group-item-info"><i class="fa fa-chevron-circle-right"></i> &nbsp;@Html.ActionLink("PRIORITY 3 - Others", "UserAcc_List", "WorkDriver")  </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </form>
    
    }

</body>
</html>