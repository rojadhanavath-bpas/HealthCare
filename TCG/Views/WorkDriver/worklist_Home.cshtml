
@{
    ViewBag.Title = "TCG - Worklist";
}
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>  </title>
    <!-- jQuery UI CSS Reference -->

</head>
<body>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()
        <form>
            <div class="container-fluid" style="max-width:90%;">
                <div class="panel panel-body">
                    <div class="form-row">
                        <div class="form-group col-sm-12">
                            <div id="id_cases" class="panel-body tab-pane fade in active">

                                <div style="border:3px solid #034EA1 ; background-color:#034EA1; color:#d9edf7;">
                                    <h4>
                                        <span style="color:#d9edf7;" class="font-weight-bold large">
                                            User Case Review
                                        </span>
                                    </h4>
                                </div>
                                <hr />

                                <div>
                                    <ul class="list-group">
                                        <li class="list-group-item list-group-item-info">
                                            <i class="fa fa-chevron-circle-right"></i> &nbsp; PRIORITY 1 - Underpayments
                                            <ul style="list-style-type:none">                                                
                                                <li>&nbsp;&nbsp;<i class="fa fa-minus-circle"></i> &nbsp;@Html.ActionLink("HCMC ", "Underpayemnts_UserAcc_List", "WorkDriver") </li>                                                
                                                <li>&nbsp;&nbsp;<i class="fa fa-minus-circle"></i> &nbsp;@Html.ActionLink("APD ", "APD_UserAcc_List", "WorkDriver") </li>
                                            </ul>
                                        </li>
                                        <li class="list-group-item list-group-item-info"><i class="fa fa-chevron-circle-right"></i> &nbsp;@Html.ActionLink("PRIORITY 2 - Denials", "UserAcc_List", "WorkDriver") </li>
                                        <li class="list-group-item list-group-item-info"><i class="fa fa-chevron-circle-right"></i> &nbsp;@Html.ActionLink("PRIORITY 3 - Others", "UserAcc_List", "WorkDriver")  </li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </form>

    }

</body>
</html>